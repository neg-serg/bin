    ${rf}â– â– â– â– ${reset}${ON}${rf}â– â– ${reset}   ${gf}â– â– â– â– ${reset}${ON}${gf}â– â– ${reset}   ${yf}â– â– â– â– ${reset}${ON}${yf}â– â– ${reset}   ${bf}â– â– â– â– ${reset}${ON}${pf}â– â– ${reset}   ${pf}â– â– â– â– ${reset}${ON}${bf}â– â– ${reset}   ${cf}â– â– â– â– ${reset}${ON}${cf}â– â– ${reset}
